<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <form action="/<%=school%>/create_event" method="POST">
        <input type="text" name="summary" placeholder="Event Name">
        <input type="text" name="description" placeholder="description">
        <p>start</p>
        <input type="date" name="startdate">
        <input type="time" name="starttime">
        <p>end</p>
        <input type="date" name="enddate">
        <input type="time" name="endtime">
        <p>participants</p>
        <% courses.forEach(function(course){ %> 
            <input type="checkbox" name="course" value="<%=course._id%>"><%=course.coursename%> 
        <% }) %> 
        <input type="text" name="general" value="true" hidden>
        <p>email reminder</p>
        <input type="number" name="emailhours" placeholder="hours" value="0">
        <input type="number" name="emailminutes" placeholder="minutes" value="0">
        <p>pop up reminder</p>
        <input type="number" name="pophours" placeholder="hours" value="0">
        <input type="number" name="popminutes" placeholder="minutes" value="0">
        <p>create google meet</p>
        <input type="radio" name="meet" value="yes">Yes
        <input type="radio" name="meet" value="no">No
        <button type="submit" name="create_event">create</button>
    </form>
</body>
</html>