<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Couse Content</title>
</head>
<body>
    <h1>Welcome to <%=found.coursename%></h1>
    <% found.items.forEach(function(item){ %>
        <label><%= item.name %></label>
        <form action="/<%=school%>/download/<%= item.name %>.<%= item.extension %>/<%= item.google_id %>" method="POST">
            <button type="submit"> download </button>
        </form>
    <% }) %>

    <h4>Professors</h4>
    <%professors.forEach(function(profs){%>
        <%=profs%><br>
            <% })%>
    
            <h4>Students</h4>
            <%students.forEach(function(student){%>
                <%=student%><br>
                    <% })%>

                    <form action="/<%=school%>/student/dashboard" method="GET">
                        <button type="submit">dashboard</button>
                    </form>

</body>
</html>